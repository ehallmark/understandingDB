<%= form_for(@member) do |f| %>
  <% if @member.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@member.errors.count, "error") %> prohibited this member from being saved:</h2>

      <ul>
      <% @member.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :hhmemberin %><br>
    <%= f.check_box :hhmemberin %>
  </div>
  <div class="field">
    <%= f.label :hhmembergen %><br>
    <%= f.number_field :hhmembergen %>
  </div>
  <div class="field">
    <%= f.label :hhmemberage %><br>
    <%= f.number_field :hhmemberage %>
  </div>
  <div class="field">
    <%= f.label :hhmemberrel %><br>
    <%= f.number_field :hhmemberrel %>
  </div>
  <div class="field">
    <%= f.label :hhmemberuasid %><br>
    <%= f.number_field :hhmemberuasid %>
  </div>
  <div class="field">
    <%= f.label :entry_id %><br>
    <%= f.select :entry, Entry.all.collect {|p| [ p.uasid, p.id ] }, {include_blank: 'None'} %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
